programa {
  funcao inicio() {
    inteiro base, altura, area

    escreva("Digite a base do seu retângulo: ")
    leia(base)

    escreva("Digite a altura do seu retângulo: ")
    leia(altura)

    area = base * altura

    escreva("A área do seu retângulo é: " + area)
  }
}

// Escreva um algoritmo para ler as dimensões de um retângulo (base e altura) e calcule a sua área.
