programa {
  funcao inicio() {
    inteiro n1, n2 

    escreva("Numero 1: ")
    leia(n1)

    escreva("Numero 2: ")
    leia(n2)

    se(n1 > n2){
      escreva("O maior numero é: " + n1)
    }senao {
      escreva("O maior numero é " + n2)
    }
  }
}

// Ler dois valores e escrever qual é o maior valor.